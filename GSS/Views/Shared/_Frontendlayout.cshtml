    <!-- wrapper end -->
    <script type="text/javascript" src="~/FrontendSection/js/_jq.js"></script>
    <script type="text/javascript" src="~/FrontendSection/js/_jquery.placeholder.js"></script>
    <script type="text/javascript" src="~/FrontendSection/js/activeaxon_menu.js"></script>
    <script type="text/javascript" src="~/FrontendSection/js/animationEnigne.js"></script>
    <script type="text/javascript" src="~/FrontendSection/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/FrontendSection/js/responsive-calendar.js"></script>


    <script type="text/javascript">
        $(document).ready(function () {

            $("#upcomming-events").vTicker();

            var currentDate = new Date();
            var day = currentDate.getDate();
            var month = currentDate.getMonth() + 1;
            var year = currentDate.getFullYear();
            var requestData = day + '-' + month + '-' + year;
            //var newdate = "\"" + requestData + "\"";
            $(".responsive-calendar").responsiveCalendar({

                onDayClick: function (events) {
                    var key;
                    key = $(this).data('year') + '-' + $(this).data('month') + '-' + $(this).data('day');
                    var data = { 'requestData': key };
                    MakeAjaxCall('/Home/GetEventbyDate', data, 'Get', fnSuccessuser, fnPostFailure);

                }

            });
        });

        function fnSuccessuser(result) {

            if (result != null) {
                document.getElementById("eventdate").innerHTML = result;
            } else {
                document.getElementById("eventdate").innerHTML = "<span class='dropcaps dropcaps-color-style'>No event found for selected date!!</span>";
            }
        }

        function fnPostFailure(result) {

            document.getElementById("eventdate").innerHTML = "<span class='dropcaps dropcaps-color-style'>No event found for selected date!!</span>";
        }



        function MakeAjaxCall(requestUrl, requestData, requestType, successFunction, failureFunction, showLoader) {
            if (showLoader)

                if (!requestUrl)
                    return;
            if (!requestData)
                requestData = "";
            if (!requestType)
                requestType = "Get";
            if (!successFunction)
                successFunction = function () { };
            if (!failureFunction)
                failureFunction = function () { };
            jQuery.ajax({
                type: requestType,
                url: requestUrl,

                data: requestData,
                cache: false,
                success: function (result) {
                    successFunction(result);
                },
                error: function (result) {
                    failureFunction();


                }
            });
        }


    </script>

    <script src="~/FrontendSection/js/ie-fixes.js" type="text/javascript"></script>
    <script src="~/FrontendSection/js/jquery.base64.js" type="text/javascript"></script>
