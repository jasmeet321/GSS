            <div class="body-wrapper">
                <div class="container">
                    <div class="row">
                        <div class="col-md-3 col-sm-3">
                            <div class="list-group icon-content-list-container">
                                <a class="list-group-item icon-content-single " id="A1" href="#">
                                    <div data-animdelay="0.2s" data-animspeed="1s" data-animrepeat="0" data-animtype="fadeIn" class="content-box  style5 small  animated  fadeIn animatedVisi" style="animation-delay: 0.2s; animation-duration: 1s;">
                                        <h4 class="h4-body-title">
                                            <strong>Latest</strong> Events
                                            <i class="icon-crown-king-streamline"></i>
                                        </h4>
                                    </div>
                                </a>
                                <a class="list-group-item icon-content-single " id="A1" href="@Url.Action(MVC.Home.About(4))">
                                    <div data-animdelay="0.2s" data-animspeed="1s" data-animrepeat="0" data-animtype="fadeIn" class="content-box  style5 small  animated  fadeIn animatedVisi" style="animation-delay: 0.2s; animation-duration: 1s;">
