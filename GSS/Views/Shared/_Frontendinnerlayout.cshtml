                                <a class="list-group-item icon-content-single " id="A1" href="@Url.Action(MVC.Home.Contact())">
                                    <div data-animdelay="0.2s" data-animspeed="1s" data-animrepeat="0" data-animtype="fadeIn" class="content-box  style5 small  animated  fadeIn animatedVisi" style="animation-delay: 0.2s; animation-duration: 1s;">
