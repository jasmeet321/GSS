@model  List<GSS.Models.Models.EventImageModel>
@{
    ViewBag.Title = "Event Gallery";
    Layout = "~/Views/Shared/_Frontendlayout.cshtml";
}


<div class="row">
    <div class="col-md-12 col-sm-12 animated" data-animtype="fadeInUp"
         data-animrepeat="0"
         data-speed="1s"
         data-delay="0.4s">
        <h2 class="h2-section-title">Event Gallery</h2>
        <div class="i-section-title">

        </div>

    </div>
</div>

<div class="section-content bottom-body">


    <div class="space-sep20"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12">
            <div class="portfolio-items portfolio-items-cols3">
                <!-- Project Description -->
                @foreach (var images in Model.ToList())
                {
                <!-- Portfolio Item -->
                    <div class="thumb-label-item branding">

                        <div class="img-overlay thumb-label-item-img">
                            <img style="height: 170px;" src="~/@images.ImagePath" alt="" />

                            <div class="item-img-overlay">
                                <a class="portfolio-zoom fa fa-plus" href="~/@images.ImagePath"
                                   data-rel="prettyPhoto[portfolio]" title="Event Gallery"></a>
                                <div class="item_img_overlay_content">
                                    <h3 class="thumb-label-item-title">
                                        <a href="~/@images.ImagePath"> Event Gallery </a>
                                    </h3>
                                </div>

                            </div>
                        </div>
                    </div>

                <!-- //Portfolio Item// -->
                <!-- Portfolio Item -->
                }
            </div>
    </div>
    <!-- //Project Description// -->
</div>



</div>