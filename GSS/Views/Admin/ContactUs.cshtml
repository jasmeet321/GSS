@model GSS.Models.Models.ContactUsModel
@{
    ViewBag.Title = "ContactUs";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<script type="text/javascript">
    check_page(5);

</script>

<div class="box">
    <div class="box-head">
        <h2>Add Contact</h2>
    </div>
    <div class="form">

        @using (Html.BeginForm("ContactUs", "Admin", FormMethod.Post, new { enctype = "multipart/form-data", id = "myform" }))
        {

            @Html.AntiForgeryToken()
            <p>
                <label>Brach Name<span>(Required Field)</span></label>
                @Html.TextBoxFor(d => d.BrachName, new { @class = "field size1" })
                @Html.ValidationMessageFor(model => model.BrachName, "", new { @class = "errors" })
            </p>
            <p>
                <label>City<span>(Required Field)</span></label>
                @Html.TextBoxFor(d => d.City, new { @class = "field size1" })
                @Html.ValidationMessageFor(model => model.City, "", new { @class = "errors" })
            </p>

            <p>

                <label>Email<span>(Required Field)</span></label>
                @Html.TextBoxFor(d => d.Email, new { @class = "field size1" })
                @Html.ValidationMessageFor(model => model.Email, "", new { @class = "errors" })
            </p>

            <p>
                <label>Address<span></span></label>
                @Html.TextAreaFor(d => d.Address, new { @class = "field size1", @cols = 30, @rows = 10 })
                @Html.ValidationMessageFor(model => model.Address, "", new { @class = "errors" })
            </p>
            <p>

                <label>Fax</label>
                @Html.TextBoxFor(d => d.Fax, new { @class = "field size1" })
                @Html.ValidationMessageFor(model => model.Fax, "", new { @class = "errors" })
            </p>
            <p>

                <label>Phone<span>(Required Field)</span></label>
                @Html.TextBoxFor(d => d.Phone, new { @class = "field size1" })
                @Html.ValidationMessageFor(model => model.Phone, "", new { @class = "errors" })
            </p>
            <p>

                <label>Pin code</label>
                @Html.TextBoxFor(d => d.Pincode, new { @class = "field size1" })
                @Html.ValidationMessageFor(model => model.Pincode, "", new { @class = "errors" })
            </p>
            <p>

                <label>State<span>(Required Field)</span></label>
                @Html.TextBoxFor(d => d.State, new { @class = "field size1" })
                @Html.ValidationMessageFor(model => model.State, "", new { @class = "errors" })
            </p>
             

            <p>
                <input type="submit" value="Submit" class="button" />
            </p>

        }
    </div>
</div>
